<%- include ('../partials/tasks/index-header') %>
<h1><%= user.name %>'s Daily Tasks</h1>
<ul>
  <% tasks.forEach(function(task, idx) { %>
    <li>
      <h2>Task #<%= idx %>: <%= task.task1 %></h2>
      <h2>Task #<%= idx %>: <%= task.task2 %></h2>
      <h2>Task #<%= idx %>: <%= task.task3 %></h2>
      <h2>Task #<%= idx %>: <%= task.task4 %></h2>
      <h2>Task #<%= idx %>: <%= task.task5 %></h2>
    </li>
  <% }); %>
</ul>


<%- include('../partials/footer') %>